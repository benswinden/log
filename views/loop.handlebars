{{#if rows}}

<table class="table">
    <thead>
        <tr>
            <td>ID</td>
            <td>Date</td>
            <td>Start Time</td>
            <td>End Time</td>
            <td>Project</td>
            <td>Notes</td>
            <td>Tags</td>
        </tr>
    </thead>
    <tbody>
        {{#each rows}}
        <tr>
            {{#each this}}
                <td>{{this}}</td>
            {{/each}}
        </tr>
        {{/each}}
    </tbody>
</table>
{{/if}}
