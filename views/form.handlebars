{{> nav}}

 <p id="editlabel">* editing *</p>

<form id="form" autocomplete="off">
    <table>
        <thead>
            <tr>
                <td>Date</td>
                <td colspan="2">Time</td>
                <td>Project</td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td><input id="date"></input></td>
                <td><input id="starttime"></input></td>
                <td><input id="endtime"></input></td>
                <td><input id="project"></input></td>
            </tr>
        </tbody>
    </table>
    <p>Notes:</p>
    <textarea id="notes"></textarea>
    <p>Tags:</p>
    <input id="tags"></input>
    <div class="submitdiv">
        <input type="submit" id="submit"></input>
        <input type="submit" id="remove" value="Remove"></input>
    </div>
</form>

